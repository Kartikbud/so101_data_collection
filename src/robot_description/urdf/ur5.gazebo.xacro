<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="ur5">

  <xacro:macro name="ur5_arm_gazebo" params="prefix">
    <gazebo>
      <plugin filename="gz_ros2_control-system" name="gz_ros2_control::GazeboSimROS2ControlPlugin">
        <parameters>$(find robot_controller)/config/ur5_controllers.yaml</parameters>
      </plugin>
    </gazebo>
  </xacro:macro>

</robot>
